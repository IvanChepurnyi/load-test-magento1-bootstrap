{
  "name": "ecomdev/magento-1-performance-bootstrap",
  "description": "Magento 1.0 performance bootstrap test",
  "license": "MIT",
  "require": {
     "ecomdev/magento-varnish": "2.0.0",
     "ecomdev/magento-1-changes": "*",
     "inchoo/php7": "*"
  },
  "require-dev": {
     "ecomdev/magento-1-data-tool": "*",
     "league/csv": "*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Inchoo/Inchoo_PHP7.git"
    },
    {
      "type": "path",
      "url": "./project"
    },
    {
      "type": "path",
      "url": "./data-tool"
    }
  ],
  "minimum-stability":"dev",
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "prepend-autoloader": true,
    "prefer-dist": true
  },
  "extra": {
    "magento-root-dir": "magento",
    "auto-append-gitignore": true
  }
}
